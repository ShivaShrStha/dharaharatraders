# Example environment variables for Dharahara Traders
# Copy this file to `.env` and edit values for production.

# Admin password (set a secure password, do NOT commit the real value)
ADMIN_PASSWORD=change_this_to_a_strong_password

# Relative path to SQLite DB used by the admin (optional)
DB_PATH=admin/dharahara_data.db

# Public site name
SITE_NAME=Dharahara Traders

# Optional: number of allowed login attempts
MAX_LOGIN_ATTEMPTS=5

# Session timeout (seconds)
SESSION_TIMEOUT=3600

# WhatsApp contact number
WHATSAPP_NUMBER=9779818852676
